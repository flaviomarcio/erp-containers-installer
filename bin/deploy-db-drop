#!/bin/bash

. ${INSTALLER_DIR}/lib/util.sh

function run()
{
  export STACK_DB_DROP=1
  log "Running ${STACK_ACTION}"
  RUN_FILE=${STACK_BIN_DIR}/${STACK_ACTION}
  chmod +x ${RUN_FILE}
}

function main()
{
  run
}


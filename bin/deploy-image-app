#!/bin/bash

. ${INSTALLER_DIR}/lib/util.sh
. ${INSTALLER_DIR}/lib/util-build.sh

function main()
{
  ACTION_LIST=(deploy-image deploy-app)
  for ACTION in "${ACTION_LIST[@]}"
  do
    log -lv "call buildPrepareProject ${ACTION} ${STACK_PROJECT}"
    buildPrepareProject ${ACTION} ${STACK_PROJECT}
  done
}

main